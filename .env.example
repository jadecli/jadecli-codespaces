# ============================================
# Environment Configuration Template
# ============================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control
# ============================================

# === API Keys ===
# Parallel Claude API key for concurrent requests
PARALLEL_APIKEY=

# Neon PostgreSQL API key (for MCP server)
NEON_API_KEY=

# Anthropic API key (if using API directly instead of Claude Code)
ANTHROPIC_API_KEY=

# GitHub token for API access
GITHUB_TOKEN=

# === Database ===
# PostgreSQL connection string for Neon
# Format: postgresql://user:password@host:port/database
DATABASE_URL=

# === Environment ===
# development | staging | production
ENVIRONMENT=development

# Enable debug mode (true/false)
DEBUG=false

# === Entity Store ===
# Cache TTL in hours
ENTITY_CACHE_TTL_HOURS=24

# Path to entity cache directory
ENTITY_INDEX_PATH=.entity-cache

# === Parallel Processing ===
# Maximum concurrent API requests (1-20)
MAX_PARALLEL_REQUESTS=5

# Request timeout in seconds
REQUEST_TIMEOUT_SECONDS=30

# === Logging ===
# DEBUG | INFO | WARNING | ERROR
LOG_LEVEL=INFO

# === Ollama Configuration ===
# Ollama server URL (default: http://localhost:11434)
OLLAMA_HOST=http://localhost:11434

# Default Ollama model for code tasks
OLLAMA_DEFAULT_MODEL=codellama:7b

# Ollama model for embeddings
OLLAMA_EMBEDDING_MODEL=nomic-embed-text

# Ollama context window size (512-32768)
OLLAMA_NUM_CTX=4096

# Ollama request timeout in seconds
OLLAMA_TIMEOUT_SECONDS=60

# === Multi-Agent Orchestration ===
# Prefer Ollama for supported tasks (true/false)
AGENT_PREFER_OLLAMA=true

# Minimum quality threshold for Ollama routing (0.0-1.0)
AGENT_QUALITY_THRESHOLD=0.7

# Enable fallback from Ollama to Claude (true/false)
AGENT_ENABLE_FALLBACK=true

# Track token usage for optimization (true/false)
AGENT_TRACK_TOKENS=true

# === Machine Identification ===
# Unique identifier for this machine (optional)
MACHINE_ID=

# Prefix for agent IDs
AGENT_ID_PREFIX=wsl
